            {{!-- {{#each user.tags as |tag|}}
            <h3>{{tag.tag_name}}</h3>
            {{/each}} --}}
            
            
            {{!-- <h4>{{tag_name}}</h4> --}}
          </section>
        </div>
        <section id="matched-users grid-item grid-item-3">
          {{#each sameTags}}
          <div class="card">
            <h3>{{tag_name}}</h3>
            {{#each users}}
            <div>
              {{username}}
            </div>
            {{/each}}
            <div>
              
            </div>
            <button type="button" class="like-button">like</button>
            <button type="button" class="comment-button">comment</button>
          </div>
          {{/each}}
        </section>
      </main>
